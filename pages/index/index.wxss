/**index.wxss**/
.contant{margin: 10rpx; height: 494rpx;background: #141414;position: relative}
.index_scan{margin-top: 40rpx;margin-left:40rpx;position: relative;width: 264rpx;height: 424rpx;overflow: hidden;border-radius: 16rpx}
.index_scan image{width: 264rpx;height: 424rpx}
.cardNumber{position: absolute;top: 70rpx;left: -60rpx;transform: rotate(-90deg);font-family: PingFangSC-Medium;font-size: 22rpx;color: #FFFFFF;letter-spacing: 0;}
.index_scan .text{position: absolute;line-height: 30rpx;color: #fff ;background: #f19abd;display:block;bottom: 0;width: 264rpx;text-align: center }
.index_project{width: 380rpx;color: #fff;margin-top:40rpx;margin-right: 20rpx; text-align: center;margin-left: 20rpx; }
.index_vip{line-height: 50rpx;font-size: 36rpx;margin-bottom: 20rpx;}
.index_rank{margin-bottom: 30rpx;}
.index_discount{line-height: 28rpx;font-size: 22rpx;margin-bottom: 60rpx;}
.index-nav{display: flex}
.index-item{flex: 1;text-align: center}
.index-item image{width: 80rpx;height: 80rpx}
.index-item text{display: block;margin-top: 40rpx;}
.index_recommend{padding: 30rpx}
.index_brand image{width: 120rpx;height: 120rpx;}
.index_mend{margin-left: 30rpx;}
.index_mend_title{line-height:46rpx;font-size: 32rpx;color: #111111;font-weight: bold;margin-bottom: 20rpx; }
.index_mend_matter{overflow: hidden;text-overflow:ellipsis;white-space: nowrap;width:540rpx;color: #666666}
.index_sts_title{padding: 28rpx 30rpx;border-bottom: 1rpx solid #f8f8f8;}
.index_star{line-height: 42rpx;font-size: 30rpx;color: #111111;font-weight: bold}
.index_more .text{color: #999999;margin-right: 10rpx;}
.index_more image{width: 10rpx;height: 20rpx;margin-top:7rpx; }
.index_wrap{width:100%; display: flex;}  
.index_wrap_item{width: 360rpx;}
.index_name{flex: 1;border-right:solid 1rpx #f8f8f8;padding: 30rpx;}
.index_name:last-child{border-right: none}
.index_name_img{width: 140rpx;height: 140rpx;border-radius: 50%;overflow: hidden}
.index_name_img image{width: 140rpx;height: 140rpx;}
.index_person{width: 190rpx;margin-left: 30rpx;}
.index_wrap_name{line-height: 46rpx;font-size: 32rpx;font-weight: bold}
.index_wrap_job{margin-top: 16rpx;}
.index_wrap_star{margin-top: 20rpx;}
.index_wrap_star image{width: 33rpx;height: 30rpx;}
.index_location{padding: 24rpx 30rpx;position: relative}
.index_location_store{font-weight: bold;margin-bottom: 20rpx;}

.index_location_time{margin-top: 4rpx;}
.index_location_distance{position: absolute;color: #999999;right: 30rpx;top:50%;margin-top: -20rpx;}

._navbar_title{line-height: 98rpx;text-align: center;font-size: 30rpx;display: flex;border-bottom: 2rpx solid #FF4C90;}
._navbar_item{flex:1;position: relative}
._navbar_icon image{width: 18rpx;height: 10rpx;position: absolute;right: 60rpx;top:40rpx;}
._navbar_item_icon image{width: 18rpx;height: 10rpx;}
._navbar_title .active{color: #FF4C90;position: relative} 
 .active::after{   position: absolute; border-left: 9px solid transparent;border-right: 9px solid transparent;border-bottom: 9px solid #FF4C90;
    content: " ";display: block; width: 0; height: 0; bottom:0;left:100rpx;}
._navbar_matter{margin-top:10rpx; }
._composite_item{width: 370rpx;position: relative;margin-right:10rpx;}
._composite_item_head{position: absolute;left: 30rpx;top:330rpx;line-height: 50rpx;color: #fff}
._composite_item_img{width: 50rpx;height: 50rpx;overflow: hidden;border-radius: 50%;margin-right: 20rpx;}
._composite_item_img image{width: 50rpx;height: 50rpx;}
._composite_item:nth-child(2){margin-right:0}
._composite_img image{width: 370rpx;height: 370rpx;}
._composite_cant{padding: 10rpx 20rpx 20rpx 24rpx;}
._composite_name{display: -webkit-box;-webkit-box-orient: vertical;-webkit-line-clamp: 2;overflow: hidden;font-size: 28rpx;color: #333333}
._composite_info{margin-top: 22rpx;color: #999999;}
._composite_price{font-size: 22rpx;}
._composite_now{color:#FF4C90}
._composite_now text{line-height: 48rpx;font-size: 30rpx;font-weight: bold}
._composite_former{margin-left: 10rpx;text-decoration:line-through}
._composite_former text{line-height: 48rpx;}
._composite_like{line-height: 48rpx;}
._lookover{line-height: 88rpx;text-align: center;color: #FF4C90;font-weight: bold;font-size: 28rpx;}

/*******店铺介绍*****/
.mendbackg{position: fixed;top:0;bottom:0;left:0;right:0;background:rgba(0, 0, 0, .6);z-index: 9}
.mendDialog{position: fixed;width: 560rpx;height: 308rpx;top:50%;margin-top: -154rpx;left: 50%;margin-left: -280rpx;background: #fff;border-radius: 8rpx;z-index: 10}
.recommendDialog_title{line-height: 80rpx;border-bottom: 1rpx solid #e5e5e5;font-size: 32rpx;text-align: center;font-weight: bold}
.recommendDialog_matter{padding: 12rpx;font-size: 26rpx;color: #666666}

/*******未领取会员卡*****/
.vipDialog_hint{padding: 50rpx 89rpx 40rpx 106rpx;text-align: center;font-size: 32rpx;color: #666666;line-height: 50rpx;}
.vipDialog_confirm{line-height: 80rpx;color: #FF4C90;border-top:#e5e5e5 1rpx solid;position: absolute;bottom: 0;text-align: center;display:block;width:560rpx; font-size: 36rpx;}

.draw{
  width: 306rpx;height: 68rpx;opacity: 0.8;border: 1px solid #FFFFFF;border-radius: 8px;line-height: 68rpx;margin: 0 auto;margin-top: 32rpx;
}
.draw_title{margin-top: 30rpx;margin-bottom: 52rpx;}
